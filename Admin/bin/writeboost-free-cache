#!/usr/bin/env python

import os
from argparse import ArgumentParser

parser = ArgumentParser()
parser.add_argument('cache_id', type=int)

args = parser.parse_args()

os.system("dmsetup message writeboost-mgr 0 switch_to %d" % (args.cache_id))
os.system("dmsetup message writeboost-mgr 0 free_cache")
