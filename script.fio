[global]
filename=/dev/mapper/perflv
#filename=/dev/vg1/backing

#loops=4
#size=32m
runtime=60
iodepth=32
ioengine=libaio

invalidate=1
#verify=sha1
thread=2
numjobs=1
direct=1

[io1]
rw=randwrite
bs=4k

[io2]
rw=randread
bs=4k

[io3]
rw=randwrite
bs=512

[io4]
rw=randread
bs=512

[io5]
rw=randwrite
bs=1024

[io6]
rw=randread
bs=1024
